#!/bin/bash

###############################################################################
# Add to PATH
###############################################################################
export PATH=$PATH:/sbin:/usr/sbin:~/bin/:~/scripts:~/Apps/eclipse:~/Apps/google_appengine
export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:/usr/lib:/lib:/usr/local/lib

###############################################################################
# Confiure terminal
###############################################################################
# Fix backspace key
stty erase ^?

###############################################################################
# CSCOPE env vars
###############################################################################
export EDITOR=vim
export CSCOPE_EDITOR=vim


###############################################################################
# Get some shell functions
###############################################################################
# project setup
. ~/scripts/project.sh

###############################################################################
# Source alias file
###############################################################################
. ~/.alias


###############################################################################
# go to current project
###############################################################################
export CURRENT_PROJECT=
cd ~/projects/${CURRENT_PROJECT}

